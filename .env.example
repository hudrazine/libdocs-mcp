# OpenRouter API Key (required)
# Set your OpenRouter API key
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Tavily API Key (optional)
# Set your Tavily API key for web search functionality
TAVILY_API_KEY=

# GitHub Personal Access Token (optional)
# Set your GitHub personal access token for enhanced GitHub features
GITHUB_PERSONAL_ACCESS_TOKEN=

# LibDocs Default Agent Model (optional)
# Specify default AI agent model (OpenRouter model ID)
LIBDOCS_DEFAULT_AGENT_MODEL=

# LibDocs Context7 Agent Model (optional)
# Specify AI model for Context7 agent
LIBDOCS_CONTEXT7_AGENT_MODEL=

# LibDocs DeepWiki Agent Model (optional)
# Specify AI model for DeepWiki agent
LIBDOCS_DEEPWIKI_AGENT_MODEL=

# LibDocs Web Search Agent Model (optional)
# Specify AI model for web search agent
LIBDOCS_WEB_SEARCH_AGENT_MODEL=

# LibDocs Web Search Limit (optional)
# Set search result limit (1-20, default: 10)
LIBDOCS_WEB_SEARCH_LIMIT=
